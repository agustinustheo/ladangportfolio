<div class="lapor-profile">
    <div class="lapor-profile-picture"></div>
    <h1 class="lapor-profile-name">
        <% if @user.profile == nil || @user.profile.fullname == '' %>
            <%= @user.username %> 
        <% else %>
            <%= @user.profile.fullname %>
        <% end %>
    </h1>
</div>


